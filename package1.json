{
  "name": "keymote",
  "version": "1.0.0",
  "description": "controll made easy!",
  "main": "./index.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "all": "electron-builder -mwl",
    "dist": "electron-builder",
    "start": "electron ."
  },
  "repository": "https://github.com/ShivamJoker/Keymote",
  "keywords": [],
  "author": "shivam kumar,zaid kesarani",
  "license": "ISC",
  "devDependencies": {
    "asar": "^0.14.3",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "robotjs": "^0.6.0",
    "ws": "^7.2.1"
  },
  "build": {
    "appId": "com.keymote",
    "productName": "Keymote",
    "files": [
      "**/*",
      "build/**/**"
    ],
    "copyright": "â’¸2020 CreativeJE",
    "dmg": {
      "icon": "build/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 220,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 350
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "postinstall": "electron-builder install-app-deps"
}